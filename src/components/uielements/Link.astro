---
import type { HTMLAttributes } from 'astro/types';

type Props = HTMLAttributes<'a'>;

const { href, class: className, ...props } = Astro.props;

---
<a href={href} class:list={["focus-visible:outline-none focus-visible:rounded focus-visible:ring-2 focus-visible:ring-tertiary", className ]} {...props}>
    <slot />
</a>
