<span class="sr-only">AmeXio</span>  

<style>
    .logo {
        overflow: visible;
        transition: transform 300ms ease-in-out;
        transform-origin: left center;

    }
    .orange {
        fill: #ff6100;
    }

    .orange, .white, .white2 {
        stroke-width: 0px;
    }

    .white {
        fill-rule: evenodd;
    }

    .white, .white2 {
        fill: #fff;
      }
  
      .spark1, .spark2, .spark3 {
          opacity: 0;
          animation: spark-down 300ms ease-in-out ;
          animation-fill-mode: forwards;
      }
      .spark1 {
          animation-delay: 200ms;
      }
  
      .spark3 {
          animation-delay: 100ms;
      }
  
      .fuse {
          transform-origin: 75% center;
          transition-delay: 300ms;
      }
    
      .logo:hover {
          transform: scale(1.008);
          
          .spark1, .spark2, .spark3 {
              opacity: 1;
              animation: spark-up 300ms ease-in-out;
              animation-fill-mode: forwards;
          }
          .spark1 {
              animation-delay: 200ms;
          }
          .spark3 {
              animation-delay: 100ms;
          }
      }

    @media (prefers-reduced-motion) {
        .spark1, .spark2, .spark3 {
            animation: none;
            opacity: 1;
        }

        .logo:hover {
             transform: unset;

            .spark1, .spark2, .spark3 {
                animation: none
            }
        }
    }
    
      @keyframes spark-up {
          from {
              transform: translate(0);
              opacity: 1;
          }
          to {
              opacity: 0;
              transform: translate(10px,-5px);
          }
      }
  
      @keyframes spark-down {
          from {
              opacity: 0;
              transform: translate(-10px,10px);
  
          }
          to {
              transform: translate(0);
              opacity: 1;
          }
      }
  </style>